setwd("~/Downloads")
# Importa a tabela de dados
dados <- read_xlsx("germinacao-media.xlsx")
#View(dados)
str(dados) # Estrutura dos dados
# Manipulação dos dados
df <- data.frame(matrix(ncol = 3, nrow = 1650)) # Cria o data frame com 3 colunas e 1650 linhas
colnames(df) <- c('tratamento', 'dia', 'nger') # Nome das colunas
df$tratamento <- rep(c(paste0(1:25, 'a'), paste0(1:25, 'b')), 33) # paste junta o número com a letra, e repete de 1 a 25, 33 vezes
df$dia <- rep(as.vector(colnames(dados[-1])), each = 50) # pega o nome das colunas de dados, exceto a primeira e repete a cada 50 vezes
df$dia <- convertToDate(df$dia) # converte para dias
df$nger <- as.numeric(stack(dados)[51:1700,1]) # empilha dos dados e guarda em uma nova coluna em df
df$AnoColeta <- rep(c(rep('a', 25), rep('b', 25)), 33) # repete a 25 vezes seguido de 25 b, 33 vezes
df %>% View # vizualizar o dataframe
# Calcula média para cada ano e geral
media_coleta <- aggregate(nger ~ AnoColeta + dia, data = df, mean)
media_geral <- aggregate(nger ~ dia, data = df, mean)
# Gráfico germinação ao longo dos dias de coleta
ggplot(df, aes(x = dia, y = nger, color = tratamento)) +
geom_line() +
geom_line(data = media_geral, aes(x = dia, y = nger, color = "Média Geral"), size = 1) +
scale_x_date(limits = c(as.Date("2023-08-29"),as.Date("2023-11-16")),
date_breaks = "2 days",
expand = c(0.01,0.01)) +
