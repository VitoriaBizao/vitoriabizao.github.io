levels=names(sort(table(Value),
decreasing=TRUE))))
ggplot(df, aes(x = Area, y = Value)) +
geom_col()
ggplot(df, aes(x = Area, y = Value)) +
geom_bar()
# GGPLOT
x <- ggplot(df, aes(x = reorder(Ares, +Value), y = Value))
x
# GGPLOT
ggplot(df, aes(x = reorder(Area, +Value), y = Value))
# GGPLOT
ggplot(df, aes(x = reorder(Area, +Value), y = Value)) + geom_bar(stat="identity", color='red',fill='red')
reorder(Area, +Value)
with(df, reorder(Area, +Value))
with(df, reorder(Area, Value))
library(dplyr)
df %>%
ggplot(aes(fct_rev(fct_reorder(Area,Value)), Value))+
geom_col() +
labs(x="Continent",title="Reversing ordered Barplot with fct_rev()")
library(tidyr)
df %>%
ggplot(aes(fct_rev(fct_reorder(Area,Value)), Value))+
